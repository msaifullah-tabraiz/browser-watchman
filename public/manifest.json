{
  "manifest_version": 3,
  "name": "Browser Watchman",
  "description": "An extension that records your history.",
  "version": "1.0.0",
  "background": {
    "service_worker": "serviceWorker.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html"
  },

  "permissions": ["activeTab", "storage", "tabs"],

  "options_page": "index.html"
}
